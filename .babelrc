{
    "presets": [
      "es2017",
      "stage-2",
      "react"
    ],
    "plugins": [
      "react-hot-loader/babel",
      "transform-decorators-legacy",
      ["transform-runtime", {
        "helpers": false,
        "polyfill": false,
        "regenerator": true,
        "moduleName": "babel-runtime"
      }]
    ]
}